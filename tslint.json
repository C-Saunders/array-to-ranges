{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "array-type": [
            true,
            "generic"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}
